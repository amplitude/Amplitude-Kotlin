<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Custom attributes for Amplitude Frustration Analytics -->
    <declare-styleable name="AmplitudeFrustrationAnalytics">
        <!-- Ignore rage click detection for this view -->
        <attr name="amplitudeIgnoreRageClick" format="boolean" />
        
        <!-- Ignore dead click detection for this view -->
        <attr name="amplitudeIgnoreDeadClick" format="boolean" />
        
        <!-- Ignore all frustration analytics for this view (convenience attribute) -->
        <attr name="amplitudeIgnoreFrustration" format="boolean" />
    </declare-styleable>
</resources>